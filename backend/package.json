{
  "name": "webds-backend",
  "version": "1.0.0",
  "description": "WebDS backend server (pkg single-file build)",
  "main": "launch.js",
  "type": "commonjs",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "node launch.js",
    "build:console": "pkg . --targets node18-win-x64 --output WebDS-Console.exe",
    "build:exe": "pkg . --targets node18-win-x64 --output WebDS.exe"
  },
  "bin": "launch.js",
  "dependencies": {
    "ws": "^8.17.0"
  },
  "pkg": {
    "targets": [ "node18-win-x64" ],
    "outputPath": "dist",
    "bin": "launch.js",
    "assets": []
  }
}
